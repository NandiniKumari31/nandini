class Node:
    def __init__(self,data):
        self.data=data
        self.next=None
def printelement(head):
    current=head
    count=0
    while current is not None:
        current=current.next
        count=count+1
    print(count)
if __name__=="__main__":
        head=Node(1)
        head.next=Node(2)
        head.next.next=Node(3)
        printelement(head)
